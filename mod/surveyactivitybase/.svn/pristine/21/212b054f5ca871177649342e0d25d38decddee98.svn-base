<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/surveyactivity/db" VERSION="20130508" COMMENT="XMLDB file for Moodle mod/surveyactivity"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
    >
    <TABLES>
        <TABLE NAME="surveyactivitybase" COMMENT="" NEXT="surveyactivity_instance">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="20" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="cmid" TYPE="int" LENGTH="20" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="course" TYPE="int" LENGTH="20" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="intro" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="introformat" TYPE="int" LENGTH="20" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="timemodified" TYPE="int" LENGTH="20" NOTNULL="true" SEQUENCE="false"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
        </TABLE>
        <TABLE NAME="surveyactivity_instance" COMMENT="The Survey Service To Lime Survey">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="20" NOTNULL="true" SEQUENCE="true" NEXT="surveyactivity_id"/>
                <FIELD NAME="base_id" TYPE="int" LENGTH="20" NOTNULL="true" SEQUENCE="false" NEXT="cmid"/>
                <FIELD NAME="cmid" TYPE="int" LENGTH="20" NOTNULL="true" SEQUENCE="false" NEXT="name"/>
                <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" COMMENT="survey activity's name" PREVIOUS="id" NEXT="description"/>
                <FIELD NAME="description" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="activity's description" PREVIOUS="name" NEXT="activity"/>
                <FIELD NAME="activity" TYPE="char" LENGTH="255" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="survey activity template's name" PREVIOUS="description" NEXT="instance_id"/>
                <FIELD NAME="instance_id" TYPE="int" LENGTH="20" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="The template's instance id" PREVIOUS="activity" NEXT="starttime"/>
                <FIELD NAME="starttime" TYPE="int" LENGTH="20" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="activity's start time line" PREVIOUS="instance_id" NEXT="endtime"/>
                <FIELD NAME="endtime" TYPE="int" LENGTH="20" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="The activity's end time" PREVIOUS="starttime" NEXT="creator_id"/>
                <FIELD NAME="creator_id" TYPE="int" LENGTH="20" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="Who create the activity The user's id." PREVIOUS="endtime" NEXT="status"/>
                <FIELD NAME="status" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="Who create the activity The user's id." PREVIOUS="creator_id" NEXT="modifiedtime"/>
                <FIELD NAME="modifiedtime" TYPE="int" LENGTH="20" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="When modified" PREVIOUS="creator_id"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
        </TABLE>
    </TABLES>
</XMLDB>