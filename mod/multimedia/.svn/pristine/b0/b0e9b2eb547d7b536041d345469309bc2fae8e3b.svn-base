<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/page/db" VERSION="20130515" COMMENT="XMLDB file for Page module"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="multimedia" COMMENT="Default comment for the table, please edit me" NEXT="multimedia_completion">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" NEXT="course"/>
        <FIELD NAME="course" TYPE="int" LENGTH="20" NOTNULL="true" SEQUENCE="false" PREVIOUS="id" NEXT="name"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" PREVIOUS="course" NEXT="intro"/>
        <FIELD NAME="intro" TYPE="text" NOTNULL="true" SEQUENCE="false" PREVIOUS="name" NEXT="introformat"/>
        <FIELD NAME="introformat" TYPE="int" LENGTH="20" NOTNULL="true" SEQUENCE="false" PREVIOUS="intro" NEXT="content"/>
        <FIELD NAME="content" TYPE="text" NOTNULL="true" SEQUENCE="false" PREVIOUS="introformat" NEXT="contentformat"/>
        <FIELD NAME="contentformat" TYPE="int" LENGTH="20" NOTNULL="true" SEQUENCE="false" PREVIOUS="content" NEXT="display"/>
        <FIELD NAME="display" TYPE="int" LENGTH="20" NOTNULL="true" SEQUENCE="false" PREVIOUS="contentformat" NEXT="displayoptions"/>
        <FIELD NAME="displayoptions" TYPE="text" NOTNULL="true" SEQUENCE="false" PREVIOUS="display" NEXT="type"/>
        <FIELD NAME="type" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" PREVIOUS="displayoptions" NEXT="completion_max"/>
        <FIELD NAME="completion_max" TYPE="int" LENGTH="20" NOTNULL="true" SEQUENCE="false" PREVIOUS="type" NEXT="completionenabled"/>
        <FIELD NAME="completionenabled" TYPE="int" LENGTH="20" NOTNULL="true" SEQUENCE="false" PREVIOUS="completion_max" NEXT="timemodified"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="20" NOTNULL="true" SEQUENCE="false" PREVIOUS="completionenabled"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="multimedia_completion" COMMENT="Default comment for the table, please edit me" PREVIOUS="multimedia">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" NEXT="mid"/>
        <FIELD NAME="mid" TYPE="int" LENGTH="20" NOTNULL="true" SEQUENCE="false" PREVIOUS="id" NEXT="userid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="20" NOTNULL="true" SEQUENCE="false" PREVIOUS="mid" NEXT="completion_now"/>
        <FIELD NAME="completion_now" TYPE="int" LENGTH="20" NOTNULL="true" SEQUENCE="false" PREVIOUS="userid"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>