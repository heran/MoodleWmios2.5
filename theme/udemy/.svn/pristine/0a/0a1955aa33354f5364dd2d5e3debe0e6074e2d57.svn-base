.path-blog .see-more-arrow-box {
    position:absolute;
    width:23px;
    height:23px;
    margin:0 0 0 697px;
    -webkit-transform-style:preserve-3d;
    -moz-transform-style:preserve-3d;
    -o-transform-style:preserve-3d;
    transform-style:preserve-3d;
    -webkit-border-radius:0 5px 0 0;
    -moz-border-radius:0 5px 0 0;
    -o-border-radius:0 5px 0 0;
    border-radius:0 5px 0 0;
    opacity:0;
    -webkit-transition-duration:.5s;
    -webkit-transition-property:all;
    -moz-transition-duration:.5s;
    -moz-transition-property:all;
    -o-transition-duration:.5s;
    -o-transition-property:all;
    transition-duration:.5s;
    transition-property:all;
}
.path-blog .see-more-arrow-box .see-more-arrow {
    background-position:-124px -86px;
    width:23px;
    height:23px;
    -webkit-border-radius:0 5px 0 0;
    -moz-border-radius:0 5px 0 0;
    -o-border-radius:0 5px 0 0;
    border-radius:0 5px 0 0;
    overflow:hidden;
}
.path-blog .see-more-arrow-box .see-more-arrow .see-more-arrow-to {
    width:23px;
    height:23px;
    background-position:-89px -66px;
    position:relative;
    margin:30px 0 0 -30px;
    -webkit-transition-duration:.5s;
    -webkit-transition-property:all;
    -moz-transition-duration:.5s;
    -moz-transition-property:all;
    -o-transition-duration:.5s;
    -o-transition-property:all;
    transition-duration:.5s;
    transition-property:all;
}
.path-blog .see-more-arrow-box.mouseover .see-more-arrow .see-more-arrow-to {
    margin:0;
}
.path-blog .see-more-arrow-box.mouseover .see-more-arrow {
    background-position:-29px -60px;
}
.path-blog .see-more-arrow-bg {
    display:block;
    margin:1px 0 0 696px;

    background-position:-29px -60px;
    width:23px;
    height:23px;
    background-position:0 -60px;
    position:absolute;
    background-color:#FFF;
    -webkit-transform-style:preserve-3d;
    -webkit-transition-duration:.5s;
    -webkit-transition-property:all;
    -webkit-transform:rotateY(-180deg) rotateZ(90deg);
    -ms-transform:rotateY(-180deg) rotateZ(90deg);
    -moz-transform-style:preserve-3d;
    -moz-transition-duration:.5s;
    -moz-transition-property:all;
    -moz-transform:rotateY(-180deg) rotateZ(90deg);
    -o-transform-style:preserve-3d;
    -o-transition-duration:.5s;
    -o-transition-property:all;
    -o-transform:rotateY(-180deg) rotateZ(90deg);
    transform-style:preserve-3d;
    transition-duration:.5s;
    transition-property:all;
    transform:rotateY(-180deg) rotateZ(90deg);
    -webkit-border-radius:0 5px 0 5px;
    -moz-border-radius:0 5px 0 5px;
    -o-border-radius:0 5px 0 5px;
    border-radius:0 5px 0 5px;
}
.path-blog .see-more-arrow-bg .see-more-arrow-mini {
    position:relative;
    margin:-1px 0 0 1px;
    width:23px;
    height:23px;
    background-position:0 -60px;
    -webkit-border-radius:0 5px 0 5px;
    -moz-border-radius:0 5px 0 5px;
    -o-border-radius:0 5px 0 5px;
    border-radius:0 5px 0 5px;
    -webkit-transform:translateZ(1px);
    -ms-transform:translateZ(1px);
    -moz-transform:translateZ(1px);
    -o-transform:translateZ(1px);
    transform:translateZ(1px);
}
.path-blog .feed-item.mouseover .see-more-arrow-box {
    margin:0 0 0 697px;
    _visibility:hidden;
    opacity:1;
}
.path-blog .feed-item.mouseover .see-more-arrow-bg {
    background:transparent;
    -webkit-transform:translateZ(1px);
    -ms-transform:translateZ(1px);
    -moz-transform:translateZ(1px);
    -o-transform:translateZ(1px);
    transform:translateZ(1px);
}
.path-blog .feed-item.un-support-3d .see-more-arrow-box {
