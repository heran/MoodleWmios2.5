.course-list-wrapper .collapse-btn {
    background:#dadada;
    background:-webkit-linear-gradient(#fafafa, #dadada);
    background:-o-linear-gradient(#fafafa, #dadada);
    background:-ms-linear-gradient(#fafafa, #dadada);
    background:linear-gradient(#fafafa, #dadada);
    border:1px solid #999;
    border-radius:4px;
    color:#656565;
    float:right;
    font-size:13px;
