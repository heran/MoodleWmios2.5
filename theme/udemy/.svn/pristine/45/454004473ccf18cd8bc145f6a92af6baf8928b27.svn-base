.mini-tooltip {
    position:relative;
    cursor:pointer;
}
.mini-tooltip .tooltip-content {
    -webkit-transition:visibility 0.2s ease 0.2s;
    -moz-transition:visibility 0.2s ease 0.2s;
    -o-transition:visibility 0.2s ease 0.2s;
    transition:visibility 0.2s ease 0.2s;
    -webkit-transition:visibility 0.2s ease 0.2s;
    -moz-transition:visibility 0.2s ease 0.2s;
    -o-transition:visibility 0.2s ease 0.2s;
    transition:visibility 0.2s ease 0.2s;
    background:#000;
    border-radius:4px;
    bottom:100%;
    color:#FFFFFF;
    cursor:default;
    display:block;
    font-size:10px;
    padding:2px 5px;
    position:absolute;
    text-align:center;
    text-indent:0;
    visibility:hidden;
    z-index:43;
    min-width:76px;
    max-width:250px;
    left:50%;
    margin-left:-38px;
}
.mini-tooltip .tooltip-content:before {
    content:'';
    width:10px;
    height:10px;
    background:#000;
    display:block;
    position:absolute;
    -moz-transform:rotate(45deg);
    -webkit-transform:rotate(45deg);
    -o-transform:rotate(45deg);
    -ms-transform:rotate(45deg);
    transform:rotate(45deg);
    -webkit-backface-visibility:hidden;
    bottom:-5px;
    left:50%;
    margin-left:-5px;
}
.mini-tooltip:hover .tooltip-content {
    visibility:visible;
}
.mini-tooltip.down .tooltip-content {
    bottom:auto;
    top:100%;
    margin-top:10px;
}
.mini-tooltip.down .tooltip-content:before {
    bottom:auto;
    top:-5px;
}
.black-tooltip {
