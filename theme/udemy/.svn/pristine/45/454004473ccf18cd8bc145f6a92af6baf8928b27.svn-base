    font-size:20px;
    position:relative;
    top:2px;
    margin-right:-3px;
}
.discover-courses-list li.course-item-list .price {
    position:absolute;
    top:0;
    right:30px;
    background:#548c1b;
    background:-webkit-linear-gradient(#699e2a, #548c1b);
    background:-o-linear-gradient(#699e2a, #548c1b);
    background:-ms-linear-gradient(#699e2a, #548c1b);
    background:linear-gradient(#699e2a, #548c1b);
    color:#fff;
    font-weight:bold;
    font-size:24px;
    padding:15px;
    bottom:20px;
    line-height:65px;
    min-width:82px;
    text-align:center;
    overflow:hidden;
}
.discover-courses-list li.course-item-list .price:before, .discover-courses-list li.course-item-list .price:after {
    content:'';
    position:absolute;
    width:51%;
    height:30px;
    background:#fff;
    left:0;
    bottom:-17px;
    -webkit-transform:skewY(-20deg);
    -o-transform:skewY(-20deg);
    -ms-transform:skewY(-20deg);
    transform:skewY(-20deg);
    -webkit-backface-visibility:hidden;
}
.discover-courses-list li.course-item-list .price:after {
    -webkit-transform:skewY(20deg);
    -o-transform:skewY(20deg);
    -ms-transform:skewY(20deg);
    transform:skewY(20deg);
    -webkit-backface-visibility:hidden;
    right:0;
    left:auto;
}
.course-box {
    display:inline-block;
    position:relative;
    vertical-align:top;
    border-radius:4px;
    overflow:hidden;
    background:#fff;
    box-shadow:0 1px 2px rgba(0, 0, 0, 0.45);
    transition:box-shadow 0.2s;
    white-space:normal;
}
.course-box:hover .add-to-wishlist {
    display:inline-block;
}
.course-box .wishlist-loader {
    display:block;
    margin-top:6px;
    z-index:2;
}
.course-box .add-to-wishlist {
    margin:10px;
    position:absolute;
    z-index:1;
    display:none;
}
.course-box .mask {
    color:#353535;
    display:block;
    font-size:small;
    width:270px;
}
.course-box .mask .title {
    display:block;
    font-size:14px;
    line-height:1.2;
    overflow:hidden;
    padding:10px;
    font-weight:bold;
}
.course-box .mask .details {
    display:block;
    overflow:hidden;
    padding:5px 10px 10px;
    text-align:center;
    line-height:10px;
}
.course-box .mask .price {
    color:#5D951F;
    float:left;
    font-size:14px;
    font-weight:bold;
    margin-top:4px;
}
.course-box .mask .stu {
    color:#555;
    float:right;
    font-weight:bold;
    margin-top:3px;
}
.course-box .mask .stu:before {
    background-position:-20px -210px;
    float:left;
    height:13px;
    margin:-2px 4px 0 0;
    width:20px;
}
.course-box .mask .small-rating {
    vertical-align:top;
}
.course-box .mask .instructors {
    border-top:1px solid #eee;
    display:block;
    margin:0 10px;
    overflow:hidden;
    padding:10px 0;
}
.course-box .mask .instructors .r {
    margin-left:33px;
    display:block;
}
.course-box .mask .ins-thumb {
    float:left;
    width:25px;
    border-radius:4px;
    margin-right:5px;
}
.course-box .mask .ins-name {
    font-size:12px;
    font-weight:bold;
    line-height:1;
}
.course-box .mask .ins-job-title {
    margin-top:2px;
    font-size:12px;
    line-height:1.2;
    font-family:georgia;
    font-weight:lighter;
    color:#555;
    font-style:italic;
}
.course-box .mask .ins-job-title>h1, .course-box .mask .ins-job-title>h2, .course-box .mask .ins-job-title>h3, .course-box .mask .ins-job-title>h4, .course-box .mask .ins-job-title>h5, .course-box .mask .ins-job-title>h6 {
    font-weight:lighter;
    font-style:italic;
    color:#555;
}
.course-box .mask .add-to-wishlist {
    position:absolute;
    top:10px;
    right:10px;
}
.course-box .mask .heading {
    float:left;
    font-style:italic;
    margin-right:6px;
    line-height:1.8;
}
.course-box .mask .two {
    padding-bottom:11px;
}
.course-box .mask .two .ins-name:first-child {
    margin-bottom:3px;
}
.course-box .mask .over-two {
    padding-bottom:13px;
}
.course-box .mask .over-two:before {
    content:'';
    right:0;
    bottom:0;
    width:25px;
    height:43px;
    background:linear-gradient(90deg, rgba(255, 255, 255, 0), rgba(255, 255, 255, 0.8), #ffffff);
    position:absolute;
}
.course-box .mask:hover {
    box-shadow:0 1px 5px rgba(0, 0, 0, 0.8);
}
.course-box .course-thumb img {
    height: 152px;
    width: 270px;
}
