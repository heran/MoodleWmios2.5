    -webkit-transition:opacity .3s ease 0.3s, visibility .3s ease 0.3s;
    -moz-transition:opacity .3s ease 0.3s, visibility .3s ease 0.3s;
    -o-transition:opacity .3s ease 0.3s, visibility .3s ease 0.3s;
    transition:opacity .3s ease 0.3s, visibility .3s ease 0.3s;
    -webkit-transition:opacity .3s ease 0.3s, visibility .3s ease 0.3s;
    -moz-transition:opacity .3s ease 0.3s, visibility .3s ease 0.3s;
    -o-transition:opacity .3s ease 0.3s, visibility .3s ease 0.3s;
    transition:opacity .3s ease 0.3s, visibility .3s ease 0.3s;
    white-space:normal;
