.date { width:318px;  clear:both;  background-color:#fdfcfc; padding:0 21px; padding-bottom:5px;}
.date li { width:51px; height:25px; float:left; margin-right:2px; cursor:pointer; *cursor:hand;background-image:url(../images/23.jpg); background-repeat:no-repeat;font-family:"微软雅黑", Arial, Helvetica, sans-serif; font-size:12px; font-weight:bold; color:#B00343; text-align:center; padding-top:3px; }
.date li.red {background-image:url(../images/22.jpg); color:#ffffff;}
.content2 { width:320px; height:410px;}
.weibo { width:320px; height:auto;  background-color:#fdfcfc; padding:20px 20px 0 20px; margin-top:10px;}
.weibo tr:nth-child(2) td:nth-child(2){color: black;font-family:"微软雅黑",Arial, Helvetica, sans-serif;}
.weibo-content{color: black;font-family:"微软雅黑",Arial, Helvetica, sans-serif;}
.box { width:320px; height:131px; overflow:hidden;background-image:url(../images/34.jpg); background-position:bottom; background-repeat:no-repeat;line-height:180%; padding-bottom:11px; word-spacing:30px; margin-bottom:10px; }
.box2 { background:none; margin-bottom: 0;}
.box a { font-size:12px; font-family:"微软雅黑",Arial, Helvetica, sans-serif; text-decoration:none;color:#AE003D;}
.box a:hover { font-size:12px; font-family:"微软雅黑",Arial, Helvetica, sans-serif; text-decoration: underline; color:#AE003D;}
.finallbox { width:320px; height:325px; background-color:#fdfcfc; padding:0px 20px 0 20px; margin-top:10px;}
.content3 { width:320px;  font-size:12px; font-family:"微软雅黑",Arial, Helvetica, sans-serif; line-height:180%;}

/* bottom*/
.Bbottom{ padding:20px 0; background:#3b3b3b; margin-top:5px;}
.Bbottom h3{ text-align:center; color:#FFF; line-height:25px; font-weight:normal;}
.Bbottom h3 a{font-size:12px; font-family:Arial, Helvetica, sans-serif; text-decoration:none;color: #FFFFFF;}
.Bbottom h3 a:hover {font-size:12px; font-family:Arial, Helvetica, sans-serif; text-decoration:none;color: #FFFFFF;}
.Bbottom h3 img{ vertical-align:middle; }
.Bbottom p{ text-align:center; color:#cae1ee; line-height:20px;}
.cbnChannels{ width:981px; margin:0 auto; border-bottom:solid 1px #5b5b5b; padding-bottom:15px; margin-bottom:15px;text-align: center;}

#slider {float:left; position:relative; overflow:auto; width:320px; height:243px; background:#000; _position:absolute;}
#slider ul {position:absolute; list-style:none; top:0; left:0}
#slider li {float:left; width:320px; height:243px}
